-    device/sample/etc/apns-full-conf.xml:system/etc/apns-conf.xml
-endif
+
 
 $(call inherit-product, $(SRC_TARGET_DIR)/product/aosp_base.mk)
 $(call inherit-product, $(SRC_TARGET_DIR)/product/telephony.mk)
